{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "privacy": {
              "type": "string"
            },
            "id": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "id"
          ]
        }
      ]
    },
    "paging": {
      "type": "object",
      "properties": {
        "cursors": {
          "type": "object",
          "properties": {
            "before": {
              "type": "string"
            },
            "after": {
              "type": "string"
            }
          },
          "required": [
            "before",
            "after"
          ]
        }
      },
      "required": [
        "cursors"
      ]
    }
  },
  "required": [
    "data",
    "paging"
  ]
}
